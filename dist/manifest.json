{
  "manifest_version": 3,
  "name": "Historial de Busqueda",
  "version": "1.0",
  "icons": {
    "16": "icons/icon.png",
    "38": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "description": "Extension de Chrome para buscar en el historial de navegacion",
  "background": {
    "service_worker": "./background.js"
  },
  "permissions": ["history", "storage", "activeTab"],
  "host_permissions": [
    "<all_urls>"
   ],
  "author": "Ezequiel Sali",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Historial de busqueda",
    "default_icon": "icons/icon.png"
  }
}